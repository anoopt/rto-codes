{
  "name": "rtocodes",
  "version": "1.0.0",
  "description": "A website to make sense of RTO (Regional Transport Office) codes in India, starting with Karnataka.",
  "main": "index.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "bun scripts/generate-index.ts && bun scripts/generate-sitemap.ts",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "generate:icons": "bun scripts/generate-icons.ts",
    "generate:sitemap": "bun scripts/generate-sitemap.ts",
    "generate:index": "bun scripts/generate-index.ts",
    "generate:images": "bun scripts/generate-rto-images.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
