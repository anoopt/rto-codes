# Feature Flags
# Set to 'true' to enable the district map highlight feature on RTO detail pages
NEXT_PUBLIC_ENABLE_DISTRICT_MAP=false

# Set to 'true' to enable OpenStreetMap-based interactive maps instead of SVG maps
# When enabled, RTO detail pages will show Leaflet/OSM maps with zoom, pan, and district boundaries
# Defaults to 'false' (uses SVG maps)
NEXT_PUBLIC_OSM_ENABLED=false

# Site URL Configuration
# Set this to your production URL for proper metadata and SEO
# Defaults to localhost:3000 for local development
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Cloudinary Configuration
# NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME is required by next-cloudinary package for client-side image rendering
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloud_name_here
CLOUDINARY_CLOUD_NAME=your_cloud_name_here
CLOUDINARY_API_KEY=your_api_key_here
CLOUDINARY_API_SECRET=your_api_secret_here

# Google Gemini API Key (for image generation script)
GEMINI_API_KEY=your_gemini_api_key_here
